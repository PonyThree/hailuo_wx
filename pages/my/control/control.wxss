.pages {
    background: #f7f7f7;
    padding-top: 1rpx;
    padding-bottom: 100rpx;
}
page{
     background: #f7f7f7 !important;
}

/* 列表信息 */

.content {
    display: flex;
    justify-content: space-between;
    width: 686rpx;
    height:217rpx;
    margin:24rpx auto 0;
    overflow: hidden;
    background: #fff;
    border-radius: 10rpx;
    box-shadow: -5px 5px 5px -4px #dcdcdc, 5px 5px 5px -4px #dcdcdc;
}
/* 左边信息 */
.content .left{
  display: flex;
  flex-direction: column;
  justify-content:center;
  width: 172rpx;
  height: 217rpx;
}
.left image{
  width: 87rpx;
  height: 99rpx;
  margin:0 40rpx 10rpx;
}
.left view{
  text-align: center;
  font-size: 28rpx;
  font-weight: bold;
}
/* 右边信息 */
.content .rightInfo{
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 514rpx;
  height: 217rpx;
  /* background-color: pink; */
  overflow: hidden;
}
.rightInfo >view{
  width: 480rpx;
  overflow: hidden;
}
.rightInfo >view .name{
  font-size: 30rpx;
  color: #1B1B1B;
  /* overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; */
  font-weight: bold;
  margin-bottom: 8rpx;
}
/* 时间 */
.rightInfo >view .time{
  width: 100%;
  font-size: 26rpx;
  color: #9F9F9F;
  padding-bottom: 10rpx;
  border-bottom:1px dashed #D4D4D4;
}
/* 规则 */
.rightInfo >view .regular{
  display: flex;
  align-items: center;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  margin-top: 15rpx;
  color: #757575;
}
.regular text:nth-child(1){
  margin-right: 10rpx;
}
/* 使用按钮 */
.rightInfo .btns text{
  position: absolute;
  width: 130rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border-radius: 28rpx;
  font-size: 26rpx;
  right: 24rpx;
}
.content .top {
    width: 102rpx;
    height: 41rpx;
    background: #feea93;
    color: #777;
    font-size: 21rpx;
    letter-spacing: 1rpx;
    text-align: center;
    line-height: 41rpx;
    border-top-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.main {
    width: 90%;
    height: 140rpx;
    margin: 6rpx auto 0;
    border-bottom: 2rpx dashed #dcdcdc;
}

