/* 无核销码页面 */

.blank {
    width: 100%;
    height: 1024rpx;
}

.blank image {
    width: 320rpx;
    height: 200rpx;
    float: left;
    margin: 170rpx 212rpx 30rpx 212rpx;
}

.blank text {
    display: block;
    width: 100%;
    color: #999;
    float: left;
    font-size: 28rpx;
    text-align: center;
    letter-spacing: 1rpx;
    margin-top: 24rpx;
    padding-left: 8rpx;
}
