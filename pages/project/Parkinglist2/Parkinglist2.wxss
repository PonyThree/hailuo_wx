/* pages/firstPage/chooseParkingA/chooseParkingA.wxss */
/* 隐藏的信息块样式 */
page {
  position: relative;
 background: white !important;
  height: 100%;
  width: 100%;
-webkit-overflow-scrolling: touch;
}
.page{
  width: 100%;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.navbar {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background:white;
}
.title-container {
  width: 200rpx;
  height: 40px;
  display: flex;
  align-items: center;
  position: absolute;
  background: red;
}
.capsule {
  position: fixed;
  left: 0;
}
.capsule i-icon{
  margin-left: 10rpx
}
.capsule .topBtn{
    width: 130rpx!important;
    height: 40rpx;
    border-radius: 20rpx;
    line-height: 40rpx;
    display: inline-block;
    padding: 0;
    font-size:12px !important;
    color: #333333;
    background-color: #eeeeee;
    margin-left: 20rpx
}
.title {
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.addClass{
  position: fixed !important;
  top: 84px;
}
/* 搜索 */
.topTime{
  height: 90rpx;
  background: #fdefcd;
  text-align: center;
  line-height:90rpx;
  font-size: 28rpx;
  color:#FB3B0F;
 
}



/* 项目介绍 */

.information {
  display: flex;
  align-items: center;
  height: 220rpx;
  background: white;
  /* border-top: 1rpx solid #f2f2f2; */
  
}

.information .img1 {
  width: 160rpx;
  height: 140rpx;
  margin-left: 20rpx;
  margin-right: 40rpx;
  border-radius: 4rpx
}
.information .img2{
  width: 30rpx;
  height: 30rpx
}
.information .img3{
  width: 18rpx;
  height: 24rpx;
  margin-right: 10rpx
}

.information>view>view {
  padding-top: 10rpx;
}
/* 筛选 */
.screen{
  margin-top: 32rpx;
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  background: white;
  z-index: 5;
  position: relative;
  font-size: 28rpx;
  color: #757575;
  border-bottom: 1rpx solid #E1E1E1;
  padding-bottom: 20rpx
}

.screenactivity{
   color: #1B1B1B !important; 
   font-weight: bold
}
.screen>view{
  flex: 2;
  font-size: 30rpx;
  line-height: 90rpx;
  overflow: hidden;

}
.screen>view:nth-child(1){
    margin-left:0;
}
.screen image{
   display: inline-block;vertical-align: middle;
 
}
.screen .dropList{
  width: 750rpx;
  background: #F8F8F8;
  height: 0;
  position: absolute;
  top:100%;
  left: 0;
  transition:  height 0.2s ;
 
}
.showdropList{
  border-top: 1rpx solid #f2f2f2;
  height: 200rpx !important;

}
.showdropList>view{
  width: 143rpx;
height: 60rpx;
border-radius: 30rpx;
background: #eeeeee;
float: left;
border: 1px solid #eeeeee;
margin-top: 10rpx;
margin-left: 10rpx;
line-height: 60rpx;
text-align: center;

}

/* 车位介绍区域样式 */

.spec {
  box-sizing: border-box;
  width: 750rpx;
  font-size: 30rpx;
  background-color: rgb(255, 255, 255);
  overflow: hidden;
  text-align: center;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #f2f2f2
}
.spec>text{
  float: left;
  padding: 0 15rpx;
  margin-left:4rpx;
  color: #B0B0B0
}
.spec image{
  width: 24rpx;
  height: 28rpx;
  text-align: center
}


/* 车位列表1样式 */
.carlist1{
  padding: 0 24rpx;
}
/* 已售出样式 */
.car1 .ycc{
  position: absolute;
  width: 63rpx;
  height: 59rpx;
  right: 0;
  top: 0
}
.cartop1{
  display: flex;
  justify-content: space-between;
  align-items: center;

}
.cartop1>view:nth-child(1){
  color: 38rpx;
  font-weight: 600;
  display: flex;
  align-items: center;

}
.cartop1 .img1{
  width:42rpx;
  height: 32rpx;
  margin-left: 10rpx
}
.cartop1>view:nth-child(2){
 font-size: 28rpx;
 color: #B0B0B0
}
.car1{
  width: 686rpx;
  background: white;
  border-radius: 10rpx;
  display: flex;
  justify-content: space-around;
  position: relative;
  align-items: center;
  height: 196rpx;
  margin-top: 20rpx;
 
}
.carimg{
  width: 166rpx;
  height: 155rpx;
  border-radius: 10rpx;

}
.car1right{
  height: 196rpx;
  width: 400rpx;
  padding: 20rpx 0;
  box-sizing: border-box
  
}
.car1right>view:nth-child(1){
  font-size: 30rpx;
  font-weight: 600
}
.car1right>view:nth-child(2){
  font-size: 24rpx;
  color: #999999

}
.car1right>view:nth-child(4){
  font-size: 28rpx;
  color: #999999
}
/* 成车位哦标签 */
.tagview {
  width: 380rpx;
  overflow-x: auto;
  white-space: nowrap;
  height: 52rpx;
  
}
 .tag {
  display: inline-block;
  background: rgb(240,240,240);
  color: #999999;
  font-size: 20rpx;
  padding: 6rpx 10rpx 6rpx 10rpx;
  letter-spacing: 2rpx;
  margin-right: 20rpx;
  border-radius: 4rpx
}
.line{
  height: 1rpx;
  width: 702rpx;
  background: #cccccc;
  position: absolute;
  bottom: -30rpx

}
