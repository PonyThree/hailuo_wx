/* 抽屉页1 */
.preventTouchMove{
    top: 0px ; 
    left:   0px ; 
    width:  750rpx; 
    height:   100% ; 
    overflow:   hidden ; 
    position:   fixed ; 
    z-index:   0 ; 
}

.demo {
  width: 550rpx;
  height: 400rpx;
  background: white;
  z-index: 999;
}
.demo .price {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
}

.demo .money {
  height: 70rpx;
  width: 490rpx;
  background: #dddddd;
  margin-left: 30rpx;
  margin-top: 20rpx;
  border-radius: 5rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
}

.demo .input {
  margin-left: 30rpx;
  height: 70rpx;
  width: 490rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  text-align: center;
}

.demo .input input:nth-child(1) {
  width: 200rpx;
  height: 70rpx;
  background: #dddddd;
}

.demo .input input:nth-child(2) {
  width: 200rpx;
  height: 70rpx;
  background: #dddddd;
}

.demo .confirm {
  width: 420rpx;
  height: 70rpx;
  border-radius: 35rpx;
  display: flex;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
  margin-top: 40rpx;
  margin-left: 75rpx;
}

.demo .confirm>view:nth-child(1) {
  width: 200rpx;
  height: 70rpx;
  border-radius: 35rpx 0 0 35rpx;
  background: #dddddd;
  border: none;
}
.demo .confirm>view:nth-child(2) {
  width: 200rpx;
  height: 70rpx;
  border-radius: 0rpx 35rpx 35rpx 0;
  background:#AB9F67;
  color: white
}
.demo .line{
  height: 1rpx;
  width: 490rpx;
  background: #f2f2f2;
  margin-left: 30rpx;
  margin-top: 60rpx;
}

