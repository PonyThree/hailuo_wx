/* 搜索 */

.page{
    overflow: hidden;
    padding: 0 32rpx;
    background: #f8f8f8
}
.top {
  height: 120rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 24rpx;
  margin-bottom: 10rpx;
}

.top .input {
  width: 500rpx;
  height: 60rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  background: white;
}

.top .input image {
  width: 21rpx;
  height: 21rpx;
  margin-left: 20rpx;
}

.top .input input {
  margin-left: 20rpx;
  width: 400rpx;
}

