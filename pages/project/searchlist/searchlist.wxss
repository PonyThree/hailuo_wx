/* 筛选列表样式 */
.shaixuan image {
  position: fixed;
  bottom: 30rpx;
  left: 0;
  right: 0;
  margin: auto;
  width: 702rpx;
  height: 80rpx;
  z-index: 10;
}

.hidBox {
  position: fixed;
  display: flex;
  justify-content: space-between;
  bottom: 30rpx;
  left: 0;
  right: 0;
  margin: auto;
  width: 703rpx;
  opacity: 1;
  height: 80rpx;
  z-index: 10;
  border-radius: 40rpx 40rpx 40rpx 40rpx;
}
.hidBox view {
  flex: 1;
  height: 80rpx;
  /* border:1px solid #00f; */
}
.hidBox>view:nth-child(1){
  flex:2
}
.hidBox>view:nth-child(2){
  flex:2
}
.hidBox>view:nth-child(3){
  flex:2
}
.hidBox>view:nth-child(4){
  flex:1
}



