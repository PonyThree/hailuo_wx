/* pages/firstPage/chooseParkingA/chooseParkingA.wxss */

/* 隐藏的信息块样式 */
page {
  position: relative;
 background: white;
  height: 100%;
  width: 100%;
-webkit-overflow-scrolling: touch;
}
.page{
  padding-bottom: 120rpx
}
input{
  border-radius: 3rpx
}
.topTime{
  height: 65rpx;
  background: #E8E4D1;
  text-align: center;
  line-height: 65rpx;
  font-size: 28rpx;
  color:white
}
.top {
  height: 120rpx;
  width: 100%;
  display: flex;
  align-items: center;
  padding-bottom: 15rpx;
  background: #f2f2f2;
}
.top .img2 {
  width: 21rpx;
  height: 21rpx;
}
.top .search {
  width: 560rpx;
  height: 60rpx;
  padding-left: 150rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  border-radius: 5rpx;
  background: white;
  margin-left: 95rpx;
  box-sizing: border-box;
  position: relative;
}

.top .search .radio {
  position: absolute;
  left: 20rpx;
  border-right: 1px solid #ccc;
  padding-right: 40rpx;
}

.top .search .img1 {
  width: 20rpx;
  height: 11rpx;
  position: absolute;
  right: 10rpx;
  top: 13rpx;
}

.top .search .img3 {
  width: 100rpx;
  height: 139rpx;
  position: absolute;
  z-index: 5;
  left: 0rpx;
  top: 70rpx;
  background-image: url(https://www.china185.com/static/image/qipaokuang.png);
  background-size: cover;
  box-shadow: 0 0 30rpx 5rpx #f2f2f2;
  text-align: center;
}

.top .search .img3>view {
  padding-top: 20rpx;
}

.top .search>input {
  height: 60rpx;
  width: 100%;
  margin-left: 20rpx;
}

/* 顶部板块固定 */

.topTitle {
  position: fixed;
  top: 0rpx;
  z-index: 1000;
  background-color: rgb(238, 238, 238);
}

/* 项目介绍 */

.information {
  display: flex;
  align-items: center;
  height: 220rpx;
  background: white;
  border-top: 1rpx solid #f2f2f2;
  
}

.information .img1 {
  width: 160rpx;
  height: 140rpx;
  margin-left: 20rpx;
  margin-right: 40rpx;
  border-radius: 4rpx
}
.information .img2{
  width: 30rpx;
  height: 30rpx
}
.information .img3{
  width: 18rpx;
  height: 24rpx;
  margin-right: 10rpx
}

.information>view>view {
  padding-top: 10rpx;
}

/* 车位介绍区域样式 */

.spec {
  box-sizing: border-box;
  width: 750rpx;
  font-size: 30rpx;
  background-color: rgb(255, 255, 255);
  overflow: hidden;
  text-align: center;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #f2f2f2
}
.spec>text{
  float: left;
  padding: 0 22rpx;
  color: #B0B0B0
}
.spec image{
  width: 24rpx;
  height: 28rpx;
  text-align: center
}
/* 车位列表1样式 */
.carlist1{
  padding: 0 24rpx
}
/* 已售出样式 */
.car1 .ycc{
  position: absolute;
  width: 702rpx;
  height: 260rpx;
  background-image: url('https://www.china185.com/static/image/ysc2x.png');
  background-position: center;
  background-size: 100% 100%;
  top: -30rpx
}
.cartop1{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 60rpx
}
.cartop1>view:nth-child(1){
  color: 36rpx;
  font-weight: 600;
  display: flex;
  align-items: center;

}
.cartop1 .img1{
  width:42rpx;
  height: 32rpx;
  margin-left: 20rpx
}
.cartop1>view:nth-child(2){
 font-size: 24rpx;
 color: #B0B0B0
}
.car1{
  display: flex;
  justify-content: space-around;
  margin-top: 60rpx;
  position: relative;
}
.carimg{
  width: 240rpx;
  height: 200rpx;
  border-radius: 5rpx;

}
.car1right{
  
  width: 400rpx
}
.car1right>view:nth-child(1){
  font-size: 36rpx;
  font-weight: 600
}
.car1right>view:nth-child(2){
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #999999

}
.car1right>view:nth-child(4){
  font-size: 28rpx;
  margin-top: 10rpx;
  color: #999999
}
/* 成车位哦标签 */
.tagview {
  width: 380rpx;
  overflow-x: auto;
  white-space: nowrap;
  height: 52rpx;
  
}
 .tag {
  display: inline-block;
  background: rgb(240,240,240);
  color: #999999;
  font-size: 20rpx;
  padding: 6rpx 10rpx 6rpx 10rpx;
  letter-spacing: 2rpx;
  margin-right: 20rpx;
  border-radius: 4rpx
}
.line{
  height: 1rpx;
  width: 702rpx;
  background: #cccccc;
  position: absolute;
  bottom: -30rpx

}
/* 车位列表2样式 ----------------------------------------------*/
.carlist2{
  padding: 0 24rpx
}
.Status{
  margin-top: 40rpx;
