<wxs module="dateUtil" src="../../../filter/dateFr.wxs"></wxs>
<view class='page'>
  <!-- 订单信息 -->
    <carlist data="{{data}}"></carlist>
  <!-- 支付成功信息 -->
  <view class='success'>
    <view class='successitem'>
      <view class='text2'>实付
        <text style='color:#A92112'>￥ {{orderlist.money}}</text>
      </view>
      <view class='text3'>您以提交认购申请,等待系统审核该订单,订单号为:{{orderNo}}</view>
      <view class='buttom'>
        <view bindtap='goindex'>返回首页</view>
        <view bindtap='subscription'>查看订单</view>
      </view>
    </view>
  </view>
</view>