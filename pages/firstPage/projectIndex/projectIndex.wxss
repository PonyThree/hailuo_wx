.wrap .item_dynamic {
  height: 205rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 43rpx 32rpx 50rpx;
}

.wrap .dynamic_wrap {
  background-color: rgb(244, 244, 244);
  width: 100%;
  height: 100%;
  padding: 0 16rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}

.wrap .item_icon {
  height: 80rpx;
  width: 82rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  background-image: url("https://china185.com/file/image/20191107/ca5e9567ccf94618b4a990bd033fcb6f.png");
  background-size: cover;
}

.wrap .item_content {
  height: 80rpx;
  width: 514rpx;
  font-size: 26rpx;
  color: #1b1b1b;
}

.wrap .item_content swiper, .wrap .item_content swiper-item {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.wrap .certificate {
  height: 354rpx;
  width: 100%;
  padding-bottom: 70rpx;
  box-sizing: border-box;
}

.wrap .certificate_content {
  height: 140rpx;
  width: 100%;
  padding: 0 32rpx;
  overflow-x: auto;
  overflow-y: hidden;
  white-space: nowrap;
  box-sizing: border-box;
}

.wrap .certificate_item {
  width: 200rpx;
  margin-right: 20rpx;
  height: 100%;
}

.wrap .certificate_item:last-of-type {
  margin-right: 0rpx;
}

.wrap .certificate_content::-webkit-scrollbar {
  display: none;
}

.wrap .notice .notice_title {
  position: relative;
}

.wrap .notice .info_icon {
  position: absolute;
  top: calc(50%);
  transform: translateY(-50%);
  right: -55rpx;
  font-weight: 400;
  width: auto;
  display: inline-block;
  height: 32rpx;
  font-size: 22rpx;
  line-height: 32rpx;
  text-align: center;
  padding: 0 10rpx;
  background-color: #ffcc4d;
  border-radius: 16rpx;
  box-sizing: border-box;
}

.wrap .list_box {
  box-sizing: border-box;
  padding: 0 32rpx;
}

.wrap .list_item {
  height: 199rpx;
  padding: 32rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #e1e1e1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wrap .list_img {
  width: 196rpx;
  height: 100%;
}

.wrap .list_content {
  width: 452rpx;
  height: 100%;
}

.wrap .list_title {
  font-size: 32rpx;
  color: #1b1b1b;
  height: 90rpx;
  width: 100%;
  line-height: 45rpx;
  margin-bottom: 12rpx;
  font-weight: bold;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.wrap .list_time {
  height: 30rpx;
  width: 100%;
  font-size: 22rpx;
  color: #757575;
}

/* 扫码组件 */

.scanCode1 {
  position: relative;
  width: 540rpx;
  height: 615rpx;
  overflow: hidden;
}

.scanCode1 image.bg1 {
  position: absolute;
  width: 540rpx;
  height: 615rpx;
  z-index: -1;
}

.scanCode1 .info1 {
  box-sizing: border-box;
  width: 100%;
  position: absolute;
  z-index: 100;
  height: 260rpx;
  top: 213rpx;
  margin: 0 auto;
  padding-top: 60rpx;
}

.scanCode1 .info1>view:nth-child(1) {
  color: rgb(51, 51, 51);
  font-size: 32rpx;
  text-align: center;
}

.info1 view.codeImg1>image {
  box-sizing: border-box;
  width: 80rpx !important;
  height: 74rpx !important;
  margin: 50rpx auto 30rpx !important;
  padding: 10rpx !important;
}

.info1 view.codeImg1 {
  margin: 0 auto;
}

.info1 view.codeImg1 image {
  display: block;
  margin: 0 auto;
}

.info1 view.codeImg1>view {
  width: 100%;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin: -15rpx auto 0;
  color: rgb(153, 153, 153);
}

view.codeImg1>view text {
  display: block;
  margin: 0 auto;
  text-align: center;
}

.scanCode1 view.btns1 {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  position: absolute;
  z-index: 100;
  bottom: 0;
  height: 80rpx;
  color: #fff;
  font-size: 30rpx;
}

.scanCode1 view.btns1>view {
  height: 100%;
  flex: 1;
  text-align: center;
  line-height: 80rpx;
}

.wrap .kefu {
  position: fixed;
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  bottom: 80rpx;
  right: 53rpx;
  box-shadow: 0 0 5px #999;
  z-index: 100;
  background-color: #fff;
  padding: 28rpx 32rpx 27rpx 33rpx;
  box-sizing: border-box;
}

.wrap .kefu .kefu_img {
  height: 100%;
  width: 100%;
  background-image: url("https://china185.com/file/image/20191108/8017db3494ad44deaf18d567959e88d3.png");
  background-size: cover;
}
