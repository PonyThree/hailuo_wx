.wrap {
  position: relative;
}

/* 分割线 */

.wrap .hr {
  height: 20rpx;
  width: 100%;
  background-color: #f3f3f3;
}

.wrap .header {
  background-color: #ffd46a;
  height: 380rpx;
  width: 100%;
  border-bottom-left-radius: 750rpx 130rpx;
  border-bottom-right-radius: 750rpx 130rpx;
  position: relative;
  padding-bottom: 25rpx;
  box-sizing: border-box;
  background-origin: content-box;
  background-clip: content-box;
}
/* 首页banner */
.projectBanner{
    width:686rpx;
    overflow: hidden;
    height: 260rpx;
    margin:0 auto;
    border-radius:5rpx;
    position: relative
}
.projectBanner swiper{
  width: 100%;
  height: 260rpx;
  border-radius:5rpx;
}

swiper-item {
  width: 100%;
 height: 260rpx;
  border-radius:5rpx;


}

swiper image {
  display: block;
  width: 100%;
  height: 100%;
  border-radius:10rpx;
 
}
.dots{ 
  position: absolute;
  right: 40rpx; 
  bottom: 20rpx; 
  display: flex; 
  justify-content: center; 
} 
.dots .dot{ 
  margin: 0 10rpx; 
  width: 14rpx; 
  height: 14rpx; 
  background: #fff; 
  border-radius: 8rpx; 
  transition: all .6s; 
} 
.dots .dot.active{ 
  width: 35rpx; 
  background: #ffffff !important ; 
}
.wrap .header .search_box {
  height: 120rpx;
  width: 100%;
  display: flex;
  align-items: center;
  padding: 30rpx 32rpx 24rpx;
  justify-content: space-between;
  box-sizing: border-box;
}

.wrap .header .city {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  position: relative;
  font-size: 30rpx;
  font-weight: bold;
  width: calc(4em + 30rpx);
  padding-right: 30rpx;
  box-sizing: border-box;
  color: #333333;
  height: 120rpx;
  line-height: 120rpx
}

.wrap .header .city::after {
  content: "";
  position: absolute;
  right: 0;
  top: calc(50%);
  transform: translateY(-5%);
  border: 9rpx solid transparent;
  border-top-color: #333;
}

.wrap .header .search {
  height: 66rpx;
  width: 510rpx;
  padding-left: 154rpx;
  position: relative;
  background-color: #fff;
  border-radius: 33rpx;
  box-sizing: border-box;
  line-height: 66rpx;
  font-size: 28rpx;
  color: #918f8a;
}

.wrap .header .search .search_icon {
  position: absolute;
  top: calc(50%);
  transform: translateY(-50%);
  left: 106rpx;
  width: 26rpx;
  height: 26rpx;
}

.wrap .carMap_box {
  width: 686rpx;
  height: 100rpx;
  /* margin: 30rpx auto; */
  margin-bottom: 30rpx;
  position: relative;
  border-radius: 10rpx;
  /* border:1px solid #000; */
}

.wrap .carMap_box .carMap {
  width: 100%;
  height: 100%;
  border-radius: 10rpx
}

.wrap .car_btn {
  height: 72rpx;
  width: 198rpx;
  text-align: center;
  line-height: 72rpx;
  color: #333;
  box-shadow: 0rpx 6rpx 5rpx rgba(164, 164, 164, 0.3);
  position: absolute;
  left: calc(50%);
  transform: translateX(-50%);
  background-color: #ffd46a;
  border-radius: 36rpx;
  bottom: -12rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.wrap .main {
  /* padding: 90rpx 32rpx 0; */
  padding: 60rpx 32rpx 0;
  box-sizing: border-box;
}

.wrap .main .open_dish_box {
  padding: 0 32rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wrap .main .open_dish_box.number {
  justify-content: center;
}

.wrap .main .open_dish_item {
  width: 270rpx;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background-size: cover;
}

.wrap .main .open_dish_true {
  background-image: url("https://china185.com/file/image/20191107/2cda7d7bf7364a4ea2257fac34d3464e.png");
}

.wrap .main .open_dish_false {
  background-image: url("https://china185.com/file/image/20191107/ca13bbcd5e2444f28bdef99758ca3eee.png");
}

.wrap .nav_box {
  margin-top: 60rpx;
  height: 165rpx;
  padding: 0 0 38rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wrap .nav_item {
  width: 104rpx;
  height: 127rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.wrap .nav_item .nav_img {
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 18rpx;
}

.wrap .nav_item .nav_title {
  width: 100%;
  height: 38rpx;
  font-size: 24rpx;
  color: #424242;
  line-height: 38rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.wrap .activityList {
  height: 323rpx;
}

.wrap .title_ {
  padding: 52rpx 35rpx 32rpx 32rpx;
  box-sizing: border-box;
  height: 137rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 38rpx;
  color: #313131;
  font-weight: bold;
}

.wrap .more {
  height: 46rpx;
  width: 94rpx;
  border: 1rpx solid #e9e9e9;
  box-sizing: border-box;
  text-align: center;
  line-height: 44rpx;
  font-size: 20rpx;
  color: #666;
  border-radius: 44rpx;
}

.wrap .activityList .activity_box {
  width: 100%;
  height: 142rpx;
  white-space: nowrap;
  overflow-x: auto;
  padding-left: 32rpx;
  overflow-y: hidden;
  box-sizing: border-box;
}

.wrap .activityList .activity_box::-webkit-scrollbar {
  display: none;
}

.wrap .activity_img {
  width: 660rpx;
  height: 100%;
  margin-right: 26rpx;
  border-radius: 16rpx;
}

.wrap .inform {
  box-sizing: border-box;
  padding: 48rpx 32rpx;
  height: 188rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wrap .inform_icon {
  width: 83rpx;
  height: 86rpx;
  border-radius: 16rpx;
  background-color: #ff7f0e;
  display: flex;
  justify-content: center;
  align-items: center;
}

.wrap .inform_img {
  width: 72rpx;
  height: 72rpx;
}

.wrap .inform_box {
  height: 90rpx;
  width: 565rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.wrap .inform_box .kpTime {
  font-size: 34rpx;
  font-weight: 900;
}

.wrap .inform_box swiper, .wrap .inform_box swiper-item {
  width: 100%;
  height: 40rpx;
}

.wrap .inform_box swiper-item {
  font-size: 28rpx;
  color: #646864;
  display: flex;
  align-items: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

