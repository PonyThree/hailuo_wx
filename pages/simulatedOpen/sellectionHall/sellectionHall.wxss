page{
    background-color: rgb(242,242,242);
}
/* 选位大厅销售情况样式 */
.saleCondition{
    width:750rpx;
    height:240rpx;
    /* padding-top:10rpx; */
    border-top:1px solid #cccccc;
    background-color: rgb(255,255,255);
    margin-bottom:20rpx;
}
.saleCondition .desc{
    display: flex;
    justify-content: space-between;
    align-items: center;
    width:702rpx;
    height:140rpx;
    margin:0 auto;
    border-bottom:1px solid rgb(204,204,204);
}
.saleCondition .desc >view.descTxt view:nth-child(1){
    font-weight:600;
    font-size:30rpx;
    color:rgb(51,51,51);
    margin-bottom: 10rpx;
}
.saleCondition .desc >view.descTxt view:nth-child(2){
    font-size:24rpx;
    color:rgb(51,51,51);
} 
.refresh{
   margin-top:-46rpx;
}
/* 刷新图片样式 */
.refresh >image{
    width:30rpx;
    height:31rpx;
    vertical-align: middle;
    margin-right:10rpx;
}
.refresh >text{
    font-size:24rpx;
    vertical-align: middle;
    color:rgb(51,51,51);
}
/* 虚拟文字介绍 */
.specTxt{
    color:rgb(169,33,18);
    text-align: center;
    width:750rpx;
    height:90rpx;
    margin:20rpx auto;
    font-size:30rpx;
    line-height: 90rpx;
    background-color: rgb(255,255,255);
}


/* 销售情况样式 */
.parkingDetail{
    width:702rpx;
    /* border:1px solid #f00; */
    margin:0 auto;
}
/* 头部介绍样式 */
.parkingDetail .detailTop{
    display: flex;
    justify-content: space-between;
    align-items: center;
    width:702rpx;
    height:90rpx;
    font-size:24rpx;
    line-height: 30rpx;
    color:rgb(51,51,51);
}
.detailTop .detailLeft{
    float:left;
    display: flex;
    justify-content: space-between;
    width:440rpx;
    height:30rpx;
    margin:30rpx 0;
}
.detailLeft .classify{
    display: flex;
    justify-content:flex-start;
}
.detailLeft .classify > view{
    width:60rpx;
    height:30rpx;
    border-radius:3rpx;
    margin-right:10rpx;
    background-color: rgb(110,152,111);
}
.detailLeft .classifyRed view{
    background-color: rgb(169,33,18);
}
.detailLeft .classifyYel view{
    background-color: rgb(219,144,0);
}
.detailRight{
    width:135rpx;
    display: flex;
    justify-content: space-between;
    height:30rpx;
}

.detailRight .correct{
    position:relative;
    width:30rpx;
    height:30rpx;
}
.detailRight view image.circleCor{
    position: absolute;
    left: 0;
    text-align: center;
    width:30rpx;
    height:30rpx;
    z-index: 3;
}
.detailRight .correct > .right{
    position:absolute;
    left:0;
    right:0;
    top:-2rpx;
    margin:auto;
    text-align: center;
    color:rgb(169,33,18);
}
/* 组团楼栋筛选样式 */
.groupBuilding{
    width:750rpx;
    margin-top:20rpx;
    background-color: rgb(255,255,255);
    
}
.groupTitle{
    display: flex;
    justify-content: space-between;
    align-items: center;
    width:750rxp;
    padding:0 24rpx;
    height:90rpx;
    line-height: 90rpx;
    font-size: 30rpx;
    
}

/* 箭头样式 */
.jiantouPic >image{
    width:20rpx;
    height:12rpx;
}
