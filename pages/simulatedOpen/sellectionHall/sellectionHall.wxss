/* 抽屉页1 */

.demo-container {
  width: 750rpx;
  background-color: rgb(255, 255, 255);
}

.demo-container .title{
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 600;

}
.demo-container .text{
font-size: 30rpx
}

.demo-container .demobox{
  margin-bottom: 20rpx;
  padding-left: 20rpx
}
.demo-container .item{
  overflow: hidden;
}

.demo-container .item>view{
  height: 60rpx;
  width: 120rpx;
  background: #dddddd;
  float: left;
  margin-left:45rpx;
  margin-top: 20rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 5rpx
}
.demo-container .confirm {
  width: 420rpx;
  height: 70rpx;
  margin: 40rpx auto;
  border-radius: 35rpx;
  display: flex;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
  padding-bottom: 20rpx
}

.demo-container .confirm>view:nth-child(1) {
  width: 200rpx;
  height: 70rpx;
  border-radius: 35rpx 0 0 35rpx;
  background: #ddd;
  border: none;
}

.demo-container .confirm>view:nth-child(2) {
  width: 200rpx;
  height: 70rpx;
  border-radius: 0rpx 35rpx 35rpx 0;
  background: #ab9f67;
  color: white;
}
/* 抽屉底部两个按钮 */
.confirm{
    display: flex;
    justify-content: space-between;
    align-items: center;
    width:400rpx;
    height:70rpx;
    margin:0 auto;
    /* background-color: #ff0; */
}
.confirm view{
    width:200rpx;
    height:70rpx;
    font-size:30rpx;
    text-align:center;
    line-height: 70rpx;
}

.confirm view:nth-child(1){
    color: rgb(119,119,119);
    background-color:rgb(221,221,221);
    border-radius:35rpx 0 0 35rpx;
}
.confirm view:nth-child(2){
    color:rgb(255,255,255);
    border-radius:0 35rpx 35rpx 0;
    background-color: rgb(171,159,103);
}
/* 楼栋列表样式 */
.parkingContent{
    width:720rpx;
    height:auto;
    margin:0 auto 20rpx;
}
.parkingContent::after{
    clear:left;
    content:'';
    display: block;
}
.parkingContent .parkList{
    float:left;
    overflow: hidden;
    box-sizing: border-box;
    width:162rpx;
    height:143rpx;
    margin-right:18rpx;
    margin-bottom:20rpx;
    border:2rpx solid rgb(110,152,111);
    border-radius:5rpx;
}
.parkingContent .parkList .parTitle{
    width:158rpx;
    height:38rpx;
    font-size:24rpx;
    color:rgb(248,248,248);
    text-align: center;
    background-color:rgb(110,152,111);
}
.parkingContent .parkList.maR0{
    margin-right:0;
}
.parkingContent .parkList .desc{
    text-align: center;
    width:158rpx;
    height:101rpx;
    line-height: 101rpx;
    font-size:32rpx;
    color:rgb(51,51,51);
}
 .parkList.parkListRed{
    border:1px solid rgb(169,33,18);
 }
 .parkList.parkListRed .parTitle {
    background-color:rgb(169,33,18);
  
 }
 .parkList.parkListYel{
    border:1px solid rgb(219,144,0);
 }
 .parkList.parkListYel .parTitle {
    background-color:rgb(219,144,0);
  
 } 
 /* 列表底部样式 */
.listBom{
    display: flex;
    justify-content: space-between;
    margin-top:10rpx;
    /* border:1px solid #000; */
}
.listBom .price text:nth-child(1){
    font-size:48rpx;
    line-height: 22rpx;
    margin-right:20rpx;
    color:rgb(169,33,18);
}
.listBom .price text:nth-child(2){
    width:59rpx;
    text-decoration: line-through;
    font-size:24rpx;
    color:rgb(153,153,153);
}
.listBom .price text.sale{
    font-size:24rpx;
    color:rgb(119,119,119);
}
.listBom view:nth-child(2){
    /* border:1px solid #f00; */
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}
.listBom view:nth-child(2) text:nth-child(1){
    float:left;
    padding-right:40rpx;
}
.listBom view:nth-child(2) text:nth-child(2){
    float:right;
    padding-right:20rpx;
}
.listBom view:nth-child(2) text{
    font-size: 24rpx;
    color:rgb(51,51,51);
    margin-top:10rpx;
}
/* 底部tabbar样式 */
.bomTab{
    display: flex;
    justify-content: space-around;
    position:fixed;
    bottom:0;
    width:750rpx;
    height:90rpx;
    background-color: rgb(255,255,255);

}
/* 收藏样式 */
.bomTab .shoucang{
    display: flex;
    height:80rpx;
    margin-top:5rpx;
    flex-direction: column;
    justify-content: center;
    /* background-color: pink; */
    align-items: center;
}
.bomTab .shoucang >image{
    width:40rpx;
    height:38rpx;
    margin-bottom:5rpx;
    
}
.bomTab .shoucang >view{
    text-align: center;
    font-size: 24rpx;
    color:rgb(51,51,51);
}
.bomTab .shoucang.active >view{
    color:rgb(171,159,103);
}
/* 选位大厅样式 */

.bomTab .choosePark {
  display: flex;
  height: 80rpx;
  margin-top: 5rpx;
  flex-direction: column;
  justify-content: center;
  /* background-color: pink; */
  align-items: center;
  background: none !important;
}

.bomTab .choosePark >image {
  width: 38rpx;
  height: 38rpx;
  margin-bottom: 5rpx;
}

.bomTab .choosePark >view {
  text-align: center;
  font-size: 24rpx;
}

.bomTab .choosePark.active >view {
  color: rgb(171, 159, 103);
}
