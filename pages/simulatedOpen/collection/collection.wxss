/* pages/opening/collection/collection.wxss */
page{
    background-color: rgb(242,242,242);
}
/* 收藏条数样式 */
.collectNum{
    box-sizing: border-box;
    width:702rpx;
    height:200rpx;
    margin:20rpx auto;
    /* border:1px solid #cccccc; */
    background-color: rgb(255,255,255);
    border-radius:10rpx;
}
.collectNum >view:nth-child(1){
    display: block;
    width:662rpx;
    margin:0 auto;
    height:100rpx;
    line-height: 100rpx;
    border-bottom:1px solid rgb(221,221,221);
    color:rgb(51,51,51);
    font-size:28rpx;
}
.collectNum >view.colTxt{
    font-size:24rpx;
    line-height: 40rpx;
    margin:10rpx 20rpx;
    color:rgb(153,153,153);
}

/* 蓝湖郡活动介绍样式 */
.desc{
    box-sizing: border-box;
    position: relative;
    width:702rpx;
    height:115rpx;
    margin:0 auto;
    padding-left:24rpx;
    color:rgb(51,51,51);
    border-radius:10rpx;
    /* border:1px solid #cccccc; */
    background-color: rgb(255,255,255);
}
.desc > view:nth-child(1){
    box-sizing: border-box;
    padding-top:24rpx;
    font-size:30rpx;
    font-weight:600;

}
.desc > view:nth-child(2){
    font-size:24rpx;
    margin-top:24rpx;
}
/* 进入大厅样式 */
.enterHall{
    display: flex;
    position:absolute;
    justify-content: center;
    top:20rpx;
    right:0;
    width:140rpx;
    height:40rpx;
    line-height: 40rpx;
    border-radius:20rpx 0 0 20rpx;
    background-color: rgb(196,188,148);
    font-size:24rpx;
    color:rgb(169,33,18);
}
.enterHall text{
    margin-left:12rpx;
}


/* 收藏选购列表标题 */
.collectTil{
    display: flex;
    align-items: center;
    justify-content: space-between;
    width:662rpx;
    height:70rpx;
    line-height: 70rpx;
    margin-bottom:20rpx;
    border-bottom:1px solid rgb(221,221,221);
}
.collectTil > text{
    height:70rpx;
    line-height: 70rpx;
    font-size:28rpx;
    color:rgb(169,33,18);
}
.collectTil >image{
    width:24rpx;
    height:24rpx;
}
/* 收藏选购列表样式 */
.recomList{
    display: flex;
    flex-direction: column;
    /* align-items: center; */
    box-sizing: border-box;
    width:702rpx;
    height:340rpx;
    padding:0 20rpx 20rpx;
    margin: 20rpx auto;
    border-radius:10rpx;
    box-shadow:-5rpx 0rpx 20rpx 0 #cccccc;
    background-color: rgb(255,255,255);
}
/* 列表头部样式 */
.recomList .listTop{
    display: flex;
    /* justify-content: space-around; */
    /* display: inline-block; */
    width:662rpx;
    height:180rpx;
    margin:0 auto 5rpx;
    padding-bottom:20rpx;
    border-bottom:1px solid rgb(221,221,221);
    overflow:hidden;
}
.listTop .listImg{
    width:160rpx;
    height:160rpx;
    margin-right:30rpx;
}
.listTop .listImg image{
    width:160rpx;
    height:160rpx;
    border-radius:12rpx;
}
.listTop .listDesc{
    position:relative;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    /* border:1px solid #000; */
}
.listTop .listDesc image{
    position:absolute;
    top:17rpx;
    right:-13rpx;
    width:32rpx;
    height:32rpx;
}
.listTil text:nth-child(1){
    color:rgb(51,51,51);
    font-size:30rpx;
    font-weight: bold;
}
.listTil text:nth-child(2){
    font-size:24rpx;
     color:rgb(51,51,51);
}   
.listDesc view:nth-child(2){
    font-size:24rpx;
    color:rgb(119,119,119);
}
.listDesc .listPic view{
    display: flex;
    align-items: center;
    justify-content: space-between;
    position:relative;
}
.listPic view{
    float:left;
}
.listDesc .listPic .listOne{
    margin-right:20rpx;
}
.listDesc .listPic .listOne image{ 
    width:100rpx;
    height:40rpx;
}
.listDesc .listPic .listOne text{
    position:absolute;
    left:14rpx;
    top:0;
    bottom:0;
    margin:auto;
    font-size:24rpx;
    color:rgb(255,255,255);
}
 .listDesc .listPic .listTwo image{
     width:120rpx;
     height:40rpx;
 }
 .listDesc .listPic .listTwo text{
    position:absolute;
    left:14rpx;
    top:0;
    bottom:0;
    margin:auto;
    font-size:24rpx;
    color:rgb(255,255,255);
}
.feature{
    display: flex;
    justify-content: space-between;
    align-items: center;
    /* background-color: #ff0; */
}
.feature  .rightBuy{
    display: flex;
    justify-content: center;
    width:140rpx;
    height:50rpx;
    margin-left:72rpx;
    margin-top:-5rpx;
    border-radius:25rpx;
    background-color: rgb(169,33,18);
    font-size:28rpx;
    line-height: 50rpx;
    color:rgb(255,255,255)!important;
}
.listFeature{
    width:246rpx;
    height:60rpx;
    /* background-color: pink; */
    overflow-x: auto;
    white-space: nowrap;
}
.listFeature text{
    display: inline-block;
    height:30rpx;
    line-height: 30rpx;
    padding:10rpx;
    font-size:24rpx;
    margin-left:10rpx;
    border-radius:5rpx;
    background-color: rgb(205,197,164);
    color:rgb(255,255,255);
}
/* 列表底部样式 */
.listBom{
    display: flex;
    justify-content: space-between;
    margin-top:10rpx;
    /* border:1px solid #000; */
}
.listBom .price text:nth-child(1){
    font-size:48rpx;
    line-height: 22rpx;
    margin-right:20rpx;
    color:rgb(169,33,18);
}
.listBom .price text:nth-child(2){
    width:59rpx;
    text-decoration: line-through;
    font-size:24rpx;
    color:rgb(153,153,153);
}
.listBom .price text.sale{
    font-size:24rpx;
    color:rgb(119,119,119);
}
.listBom view:nth-child(2){
    /* border:1px solid #f00; */
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}
.listBom view:nth-child(2) text:nth-child(1){
    float:left;
    padding-right:40rpx;
}
.listBom view:nth-child(2) text:nth-child(2){
    float:right;
    padding-right:20rpx;
}
.listBom view:nth-child(2) text{
    font-size: 24rpx;
    color:rgb(51,51,51);
    margin-top:10rpx;
}
/* 底部tabbar样式 */
/* 底部tabbar样式 */
.bomTab{
    display: flex;
    justify-content: space-around;
    position:fixed;
    bottom:0;
    width:750rpx;
    height:90rpx;
    background-color: rgb(255,255,255);

}
/* 收藏样式 */
.bomTab .shoucang{
    display: flex;
    height:80rpx;
    margin-top:5rpx;
    flex-direction: column;
    justify-content: center;
    /* background-color: pink; */
    align-items: center;
    background: none !important
}
.bomTab .shoucang >image{
    width:40rpx;
    height:38rpx;
    margin-bottom:5rpx;
    
}
.bomTab .shoucang >view{
    text-align: center;
    font-size: 24rpx;
    color:rgb(51,51,51);
}
.bomTab .shoucang.active >view{
    color:rgb(171,159,103);
}
/* 选位大厅样式 */
.bomTab .choosePark{
    display: flex;
    height:80rpx;
    margin-top:5rpx;
    flex-direction: column;
    justify-content: center;
    /* background-color: pink; */
    align-items: center;
}
.bomTab .choosePark >image{
    width:38rpx;
    height:38rpx;
    margin-bottom:5rpx;
}
.bomTab .choosePark >view{
    text-align: center;
    font-size: 24rpx;
}
.bomTab .choosePark.active >view{
    color:rgb(171,159,103);
}
.box{
    width:750rpx;
    height:80rpx;
    padding:5rpx;
}